{
    "name": "yish/laravel-accessible-ip",
    "description": "Accessible IPs for Laravel.",
    "license": "MIT",
    "keywords": ["laravel", "ip"],
    "authors": [
        {
            "name": "Yish",
            "email": "mombuartworks@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "phpunit/phpunit": "5.7.*",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
        "mockery/mockery": "^0.9.9"
    },

    "require-dev": {
        "laravel/laravel": "5.2.*|5.3.*|5.4.*"
    },

    "autoload": {
        "psr-4": {
            "Mombuyish\\AccessibleIP\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Mombuyish\\AccessibleIP\\Tests\\": "tests/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
